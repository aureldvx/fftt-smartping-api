{
	"name": "aureldvx/fftt-api-v2",
	"description": "Client PHP pour communiquer avec l'API FFTT",
	"version": "1.0.0",
	"type": "library",
	"license": "MIT",
	"authors": [
		{
			"name": "AurÃ©lien Devaux",
			"email": "37188045+aureldvx@users.noreply.github.com"
		}
	],
	"require": {
		"php": ">=8.1",
		"ext-curl": "*",
		"nesbot/carbon": "3.11.0"
	},
	"require-dev": {
		"laravel/pint": "1.26.0",
		"pestphp/pest": "4.2.0",
		"phpstan/phpstan": "2.1.33",
		"rector/rector": "2.3.0"
	},
	"autoload": {
		"psr-4": {
			"SmartpingApi\\": "src/"
		}
	},
	"minimum-stability": "stable",
	"prefer-stable": true,
	"config": {
		"sort-packages": true,
		"preferred-install": "dist",
		"allow-plugins": {
			"pestphp/pest-plugin": true
		}
	},
	"scripts": {
		"lint": "pint --test --parallel",
		"refactor": "rector --dry-run",
		"analyze": "phpstan analyse --ansi --memory-limit=1G",
		"test": "pest --colors=always --coverage --parallel"
	}
}
